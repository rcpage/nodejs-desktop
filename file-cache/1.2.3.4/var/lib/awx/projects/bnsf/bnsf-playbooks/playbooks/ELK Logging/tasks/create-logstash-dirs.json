{
  "filename": "create-logstash-dirs.json",
  "task": {
      "name": "Create logstash directories",
      "become": "yes",
      "tags": [
        "create",
        "logstash",
        "dirs"
      ],
      "with_items": "{{ LOGSTASH_DIRS }}",
      "ignore_errors": "yes",
      "file": {
        "path": "{{item}}",
        "state": "directory"
      }
  },
  "path": "./tasks/create-logstash-dirs.json"
}